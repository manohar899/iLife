.multidiv {
    float: left;
    width: 28%;
	 margin: 2%;
    border: 1px solid;
    background-color: #CED8F6;
    text-align:center;
}
.multidivbig {
    float: left;
    width: 29%;
    height: 100%;
	 margin: 1%;
    text-align:center;
}
.multidivnobor {
    float: left;
    width: 28%;
    height: 20%;
	 margin: 2%;
    text-align:center;
}
.multidivdesc {
    float: left;
    width: 45%;
    height: 20%;
	 margin: 2%;
    text-align:center;
}

.multidiv:hover {
	-moz-box-shadow: 0 0 10px rgba(0,0,0,0.5);
	-webkit-box-shadow: 0 0 10px rgba(0,0,0,0.5);
	box-shadow: 0 0 10px rgba(0,0,0,0.5);
	}
.multidivbig:hover {
	-moz-box-shadow: 0 0 10px rgba(0,0,0,0.5);
	-webkit-box-shadow: 0 0 10px rgba(0,0,0,0.5);
	box-shadow: 0 0 10px rgba(0,0,0,0.5);
	}
.centerbox{
    text-align : center;
    width: 100%;
    height: 100%;
	border: 1px solid;
}
.list{
	text-align: left;
}
.padd{
	padding-right:100px;
}
.valign{
	vertical-align: text-top;
}
.balign{
	vertical-align: text-bottom;
}
.ralign{
	text-align : right;
}
